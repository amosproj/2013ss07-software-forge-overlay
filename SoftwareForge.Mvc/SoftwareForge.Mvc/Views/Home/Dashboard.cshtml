@using SoftwareForge.Common.Models
@model System.Collections.Generic.List<SoftwareForge.Common.Models.Project>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
    <head>
        <title>Dashboard</title>
    </head>
    <body>
        <div>
            <h3>Noch nicht beigetretene Projekte:<br/></h3>             
                @foreach (Project p in Model)
                {
                    <b>@p.Name<br/></b>
                }
              
        </div>
    </body>
</html>