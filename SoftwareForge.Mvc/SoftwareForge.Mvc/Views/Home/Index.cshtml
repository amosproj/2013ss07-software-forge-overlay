@using SoftwareForge.Common.Models
@model IEnumerable<TeamCollection>
@{
    ViewBag.Title = "Home";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                Welcome to the Software Forge!
            </p>
        </div>
    </section>
}
<h3>Overview</h3><br/>

@foreach (TeamCollection teamCollection in Model)
{
    
    <b>@teamCollection.Name</b> 
          
    <ul>
    @foreach (Project project in teamCollection.Projects)
    {
        <li>
            @project.Name
        </li>
    }
    </ul>
}
    
<div>
    @Html.ActionLink("Create new TeamCollection","Create","TeamCollections")
</div>